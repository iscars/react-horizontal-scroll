  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@vitejs/plugin-react": "^1.3.0",
    "sass": "^1.50.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.2"
  }
